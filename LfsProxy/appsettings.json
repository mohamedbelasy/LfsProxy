{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "BasePath": "lfs-server",
  "Lfs": {
    "StoragePath": "./locks",
    "S3": {
      "Endpoint": "http://127.0.0.1:9000",
      "Region": "us-east-1",
      "S3ForcePathStyle": true,
      "AccessKey": "lfs-user",
      "SecretKey": "lfs-password",
      "BucketName": "git-lfs-test",
      "UseSsl": false,
      "RootPathPrefix": "lfs/objects",
      "PerRepoStorage": true
    },
    "PresignedUrlExpirySeconds": 3600,
    "Users": [
      {
        "Uid": "01234567-8910-1112-1314-151617181920",
        "Username": "user",
        "Password": "pswd"
      }
    ]
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5074"
      }
    }
  }
}
